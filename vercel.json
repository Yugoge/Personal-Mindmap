{
  "version": 2,
  "functions": {
    "api/render.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/md/(.*)",
      "dest": "/api/render.js",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}

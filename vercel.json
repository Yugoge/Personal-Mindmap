{
  "version": 2,
  "builds": [
    {
      "src": "api/render.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["docs/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/md/(.*)",
      "dest": "/api/render?path=$1",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "regions": ["iad1"], 
  "functions": {
    "api/render.js": {
      "memory": 3008,
      "maxDuration": 15
    }
  }
}